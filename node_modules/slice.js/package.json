{
  "name": "slice.js",
  "version": "1.1.1",
  "description": "nano version for slice of string and array like Python.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "babel src --out-dir lib && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hustcc/slice.js.git"
  },
  "keywords": [
    "slice.js",
    "slice"
  ],
  "author": "hustcc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hustcc/slice.js/issues"
  },
  "homepage": "https://github.com/hustcc/slice.js#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "jest": "^21.2.1"
  },
  "jest": {
    "testRegex": "(/test)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  }
}
